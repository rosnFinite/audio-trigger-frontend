{
  "name": "LVRF",
  "version": "0.1.0",
  "author": "Fabio Rose",
  "description": "Webanwendung für die Durchführung und Visualisierung von Stimmfeldaufnahmen",
  "homepage": "./",
  "main": "public/electron-main.js",
  "private": true,
  "proxy": "http://localhost:5001/",
  "type": "module",
  "dependencies": {
    "@mantine/core": "^7.4.1",
    "@mantine/hooks": "^7.4.1",
    "@mantine/notifications": "^7.5.3",
    "@nivo/heatmap": "^0.84.0",
    "@reduxjs/toolkit": "^2.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.70",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "axios": "^1.6.7",
    "electron-is-dev": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "^5.0.1",
    "redux-persist": "^6.0.0",
    "socket.io": "^4.7.3",
    "socket.io-client": "^4.7.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron-main.js",
    "preelectron-pack": "react-scripts build",
    "electron": "electron .",
    "electron-build": "npm run build && electron-builder -c.extraMetadata.main=build/electron-main.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/css-modules": "^1.0.5",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.1.5",
    "electron-builder": "^24.13.3",
    "wait-on": "^7.2.0"
  },
  "build": {
    "win": {
      "icon": "build/logo512.png"
    }
  }
}
